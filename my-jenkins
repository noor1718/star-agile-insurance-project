pipeline {
  agent any

  tools {
      maven 'M2_HOME'
    }
  stages {
    
    stage('Checkout') {
       steps {
         echo 'Checkout the code from GitRepo'
         git  'https://github.com/Fatima-Ishrath021/star-agile-insurance-project.git'
	 }
       }
  stage('Building the Application'){
        steps {
	echo "Cleaning... Compiling...Testing... Packaging..."
	sh 'mvn clean package'
            }
          }   
  
  
  
  }
}
